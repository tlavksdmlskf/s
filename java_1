
public class TNDJQ1 {

	public static void main(String[] args) {
		int a;
		int b;
		a=18;
		b=99;
		System.out.println("a는 "+a+"이고 b는 "+b+"입니다");
		System.out.println("값을 바꾸겠읍니다~");
		int c=a;
		a=b;
		b=c;
		System.out.println("a는 "+a+"이고 b는 "+b+"입니다");
		int x =1;
		int y=2;
		int z=3;
		int aa;
		aa=x;
		x=y;
		y=z;
		z=aa;
		System.out.println(x);
		System.out.println(y);
		System.out.println(z);
	}

}
